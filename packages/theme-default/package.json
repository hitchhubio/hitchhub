{
  "name": "@hitchhub/theme-default",
  "version": "0.1.1",
  "description": "The hitchhub default theme.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./theme.css": "./dist/theme.css",
    "./light.css": "./dist/light.css",
    "./dark.css": "./dist/dark.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier 'src/**/*.ts' --write",
    "format:check": "prettier 'src/**/*.ts' --check",
    "lint": "eslint 'src/**/*.ts'",
    "build": "pnpm build:tokens && pnpm build:source",
    "build:tokens": "tsx scripts/build.ts",
    "build:source": "tsup"
  },
  "prettier": "@aaos/prettier-config",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@hitchhub/theme-builder": "workspace:*",
    "@tsconfig/node20": "^20.1.5",
    "eslint": "~9.27.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "5.8.3"
  }
}
