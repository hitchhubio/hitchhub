{
  "name": "@hitchhub-react/ds",
  "version": "0.1.1",
  "description": "The hitchhub collection of components.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier 'src/**/*.{ts,tsx}' --write",
    "format:check": "prettier 'src/**/*.{ts,tsx}' --check",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "build": "pnpm build:ts && pnpm build:styles",
    "build:ts": "tsup",
    "build:styles": "tailwindcss -i ./styles.css -o ./dist/styles.css --minify",
    "watch": "tailwindcss -i ./styles.css -o ./dist/styles.css --watch=always",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "prettier": "@aaos/prettier-config",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@hitchhub/tokens": "workspace:*"
  },
  "devDependencies": {
    "@aaos/eslint-config": "^0.4.0",
    "@aaos/nx-publish": "^1.4.0",
    "@aaos/prettier-config": "^0.1.4",
    "@chromatic-com/storybook": "^1.6.1",
    "@hitchhub/theme-default": "workspace:*",
    "@hitchhub-react/ds": "workspace:*",
    "@hitchhub-react/wireframe": "workspace:*",
    "@storybook/addon-a11y": "^8.2.7",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-interactions": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/addon-onboarding": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/react": "^8.2.7",
    "@storybook/react-vite": "^8.2.7",
    "@storybook/test": "^8.2.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.8.0",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.2.7",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.2.4",
    "typescript": "5.4.5",
    "vite": "^5.3.5"
  }
}
