    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@aaos/eslint-config": "0.5.0",
    "@aaos/prettier-config": "0.5.0",
    "@hitchhub/ts-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "prettier": "^3.5.3",
